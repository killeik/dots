#Input devices configuration


#Keyboard
input "1:1:AT_Translated_Set_2_keyboard" {
    xkb_layout "us,ru"
    xkb_model "pc105"
    xkb_options "grp:alt_shift_toggle"
    xkb_numlock enable
}



#Touchpad
input "2:7:SynPS/2_Synaptics_TouchPad" {
     dwt off
     tap enabled
     natural_scroll enabled
     middle_emulation enabled
}
